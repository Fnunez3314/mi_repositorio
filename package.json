{
  "title": "Arctic Monkeys-Fan Page",
  "name": "proyecto-final",
  "version": "7.0.3",
  "scripts": {
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node scripts/build-assets.js",
    "build:pug": "node scripts/build-pug.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js",
    "build-css": "node-sass --include-path scss src/scss/styles.scss index/css/styles.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""

  },
  "description": "Fan page dedicada a la banda británica Arctic Monkeys",
  "keywords": [
    "Arctic Monkeys",
    "banda británica",
    "rumine",
    "brainstorm",
    "fan page",
    "proyecto final"
  ],
  "homepage": "https://startbootstrap.com/theme/creative",
  "bugs": {
    "url": "https://github.com/StartBootstrap/startbootstrap-creative/issues",
    "email": "fnunez3314@gmail.com"
  },
  "license": "MIT",
  "author": "Federico Nuñez",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fnunez3314/mi_repositorio.git"
  },
  "dependencies": {
    "bootstrap": "5.0.2"
  },
  "devDependencies": {
    "autoprefixer": "10.2.6",
    "browser-sync": "2.27.3",
    "chokidar": "3.5.2",
    "concurrently": "6.2.0",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "pug": "3.0.2",
    "sass": "1.35.1",
    "shelljs": "0.8.4",
    "upath": "2.0.1"
  },
  "main": "index.js"
}
